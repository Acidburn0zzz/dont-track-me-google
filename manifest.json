{
    "name": "Don't track me Google",
    "description": "Removes the annoying link-conversion at Google Search/maps/...",
    "version": "4.0",
    "manifest_version": 2,
    "content_scripts": [{
        "run_at": "document_end",
        "js": ["contentscript.js"],
        "matches": [
            "*://*.google.com/*"
        ]
    }],
    "icons": {
        "48": "icon48.png",
        "96": "icon96.png"
    },
    "applications": {
        "gecko": {
            "id": "dont-track-me-google@robwu.nl"
        }
    }
}
